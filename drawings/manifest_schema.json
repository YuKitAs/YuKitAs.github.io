{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "Manifest Schema for Drawings",
    "default": {},
    "additionalProperties": true,
    "required": [
        "data"
    ],
    "properties": {
        "data": {
            "$id": "#/properties/data",
            "type": "array",
            "title": "The Data Schema",
            "default": [],
            "examples": [
                [
                    {
                        "drawings": [
                            {
                                "name": "smartphone-1",
                                "year": "2015",
                                "id": 1
                            },
                            {
                                "name": "smartphone-2",
                                "year": "2015",
                                "id": 2
                            }
                        ],
                        "name": "smartphone"
                    },
                    {
                        "drawings": [
                            {
                                "year": "2011",
                                "id": 1,
                                "name": "sketch-1"
                            },
                            {
                                "name": "sketch-2",
                                "date": "2011",
                                "id": 2
                            }
                        ],
                        "name": "sketch"
                    }
                ]
            ],
            "additionalItems": true,
            "items": {
                "$id": "#/properties/data/items",
                "type": "object",
                "title": "The Items Schema",
                "default": {},
                "examples": [
                    {
                        "drawings": [
                            {
                                "year": "2015",
                                "id": 1,
                                "name": "smartphone-1"
                            },
                            {
                                "name": "smartphone-2",
                                "year": "2015",
                                "id": 2
                            }
                        ],
                        "name": "smartphone"
                    },
                    {
                        "drawings": [
                            {
                                "year": "2011",
                                "id": 1,
                                "name": "sketch-1"
                            },
                            {
                                "name": "sketch-2",
                                "date": "2011",
                                "id": 2
                            }
                        ],
                        "name": "sketch"
                    }
                ],
                "additionalProperties": true,
                "required": [
                    "name",
                    "drawings"
                ],
                "properties": {
                    "name": {
                        "$id": "#/properties/data/items/properties/name",
                        "type": "string",
                        "title": "The Name Schema",
                        "default": "",
                        "examples": [
                            "smartphone"
                        ]
                    },
                    "drawings": {
                        "$id": "#/properties/data/items/properties/drawings",
                        "type": "array",
                        "title": "The Drawings Schema",
                        "default": [],
                        "examples": [
                            [
                                {
                                    "year": "2015",
                                    "id": 1,
                                    "name": "smartphone-1"
                                },
                                {
                                    "year": "2015",
                                    "id": 2,
                                    "name": "smartphone-2"
                                }
                            ]
                        ],
                        "additionalItems": true,
                        "items": {
                            "$id": "#/properties/data/items/properties/drawings/items",
                            "type": "object",
                            "title": "The Items Schema",
                            "default": {},
                            "examples": [
                                {
                                    "year": "2015",
                                    "id": 1,
                                    "name": "smartphone-1"
                                },
                                {
                                    "year": "2015",
                                    "id": 2,
                                    "name": "smartphone-2"
                                }
                            ],
                            "additionalProperties": true,
                            "required": [
                                "id",
                                "name",
                                "year"
                            ],
                            "properties": {
                                "id": {
                                    "$id": "#/properties/data/items/properties/drawings/items/properties/id",
                                    "type": "integer",
                                    "title": "The Id Schema",
                                    "description": "An explanation about the purpose of this instance.",
                                    "default": 0,
                                    "examples": [
                                        1
                                    ]
                                },
                                "name": {
                                    "$id": "#/properties/data/items/properties/drawings/items/properties/name",
                                    "type": "string",
                                    "title": "The Name Schema",
                                    "default": "",
                                    "examples": [
                                        "smartphone-1"
                                    ]
                                },
                                "year": {
                                    "$id": "#/properties/data/items/properties/drawings/items/properties/year",
                                    "type": "string",
                                    "title": "The Year Schema",
                                    "default": "",
                                    "examples": [
                                        "2015"
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
